<launch>
	<arg name="world" default="singlePillar"/>

	<include file="$(find husky_gazebo)/launch/husky_empty_world.launch">
		<arg name="world_name" value="$(find husky_highlevel_controller)/worlds/$(arg world).world"/>
	</include>

	<node name="husky_highlevel_controller" pkg="husky_highlevel_controller" type="husky_highlevel_controller.py"/>
	<node name="husky_broadcaster" pkg="husky_highlevel_controller" type="husky_broadcaster.py"/>
	
</launch>